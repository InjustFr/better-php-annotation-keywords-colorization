{
    "name": "better-php-annotation-keywords-colorization",
    "displayName": "Better php annotation keywords-colorization",
    "description": "Allows recognition of php tools keywords in annotations besides phpdoc",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.70.0"
    },
    "categories": [
        "Programming Languages",
        "Other"
    ],
    "contributes": {
        "grammars": [
            {
                "path": "./syntaxes/php.tmLanguage.json",
                "scopeName": "php.injection",
                "injectTo": [
                    "text.html.php"
                ]
            }
        ]
    }
}
