    "repository": {
        "type": "git",
        "url": "https://github.com/InjustFr/better-php-annotation-keywords-colorization.git"
    },
    "publisher": "injust",
    "version": "1.0.0",
    "engines": {
        "vscode": "^1.70.0"
    },
    "categories": [
        "Programming Languages",
        "Other"
    ],
    "contributes": {
        "grammars": [
            {
                "path": "./syntaxes/php.tmLanguage.json",
                "scopeName": "php.injection",
                "injectTo": [
                    "text.html.php"
                ]
            }
        ]
    }
}
